<button id="btn-loadTest" class="btn btn-primary add-padding"><span class="glyphicon glyphicon-repeat"></span> Load Data</button>

<div id="payload-list" data-bind="with:payloadList">
    <table class="table table-striped table-bordered">

        <thead>
            <tr>
                <th>Title</th>
                <th>Slug</th>
                <th>TV Channel</th>
                <th>Country</th>
                <th>Drm</th>
                <th>EpisodeCount</th>
                <th>Genre</th>
            </tr>
        </thead>

        <tbody data-bind="foreach:payload">
            <tr>
                <td>
                    <span data-bind="text:title"></span>
                </td>
                <td>
                    <span data-bind="text:slug"></span>
                </td>
                <td>
                    <span data-bind="text:tvChannel"></span>
                </td>
                <td>
                    <span data-bind="text:country"></span>
                </td>
                <td>
                    <span data-bind="text:drm"></span>
                </td>
                <td>
                    <span data-bind="text:episodeCount"></span>
                </td>
                <td>
                    <span data-bind="text:genre"></span>
                </td>
                <td>
                    <button class="btn btn-sm btn-danger btn-delete-entry"><span class="glyphicon glyphicon-trash"></span> Delete</button>
                </td>
            </tr>
        </tbody>

    </table>
</div>